{
  "name": "exchangerate",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}
